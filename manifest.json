{
   "author": "Rosen Suvariev \u003Crsuvariev@i-covereurope.com>",
   "background": {
      "persistent": true,
      "scripts": [ "jquery.js", "background.js", "context-menu.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icon_19x19.png"
      },
      "default_title": "The Test Mart Add Element"
   },
   "content_scripts": [ {
      "js": [  "jquery.js","contentscript.js" ],
      "matches": [ "\u003Call_urls>" ]
	}
   ],
   "description": "Toggle active state by clicking the extention icon. Then click on the element you would like to add, name it then toggle the state once completed",
   "icons": {
      "128": "icon_128x128.png",
      "16": "icon_16x16.png",
      "19": "icon_19x19.png",
      "32": "icon_32x32.png",
      "38": "icon_38x38.png",
      "48": "icon_48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "49.0.0",
   "name": "The Test Mart Add Element",
   "permissions": [ "activeTab", "contextMenus", "storage", "webRequest", "webRequestBlocking", "*://*.testmartsetparams.com/*"],
   "version": "0.0.6.0"
}
